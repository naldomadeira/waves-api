{
    "name": "waves-api",
    "version": "1.0.0",
    "description": "REST API to calculate the best conditions for practicing sports among several beaches",
    "main": "index.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint ./src ./test --ext .ts",
        "lint:fix": "eslint ./src ./test --ext .ts --fix",
        "start": "yarn build && node dist/src/index.js",
        "test": "yarn test:unit && yarn test:functional",
        "start:dev": "ts-node-dev 'src/index.ts'",
        "test:functional": "jest --projects ./test --runInBand",
        "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
        "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
        "test:unit": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/naldomadeira/waves-api.git"
    },
    "keywords": [
        "nodejs",
        "typescript",
        "api"
    ],
    "author": "Naldo Madeira",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/naldomadeira/waves-api/issues"
    },
    "homepage": "https://github.com/naldomadeira/waves-api#readme",
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^5.0.0",
        "@types/config": "^0.0.41",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/module-alias": "^2.0.1",
        "@types/mongoose": "^5.11.97",
        "@types/nock": "^11.1.0",
        "@types/node": "14",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "eslint": "^8.5.0",
        "jest": "^27.4.5",
        "nock": "^13.2.2",
        "prettier": "^2.5.1",
        "supertest": "^6.1.6",
        "ts-jest": "^27.1.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@overnightjs/core": "^1.7.6",
        "axios": "^0.25.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "config": "^3.3.7",
        "express": "^4.17.2",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "mongoose": "^6.1.8"
    }
}
