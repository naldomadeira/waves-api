{
  "name": "waves-api",
  "version": "1.0.0",
  "description": "REST API to calculate the best conditions for practicing sports among several beaches",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "test:functional": "jest --projects ./test --runInBand",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naldomadeira/waves-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Naldo Madeira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naldomadeira/waves-api/issues"
  },
  "homepage": "https://github.com/naldomadeira/waves-api#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "module-alias": "^2.2.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
